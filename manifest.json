{
  "name": "Tasty Backspace",
  "version": "1.0.2",
  "description": "Ever wonder where those backspaced letters were going?",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://*/*","file:///*"],
      "css": ["tasty.css"],
      "js": ["hungry-hungry-backspace.js"],
      "all_frames": false
    }
  ],
  "icons": {
    "16": "images/icons/16.png",
    "32": "images/icons/32.png",
    "48": "images/icons/48.png",
    "64": "images/icons/64.png",
    "128": "images/icons/128.png"
  }
}
